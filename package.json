{
  "name": "dha-digital-services",
  "version": "2.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=9.0.0"
  },
  "engineStrict": false,
  "scripts": {
    "dev": "node --loader ts-node/esm server/index.ts",
    "build": "tsc --project tsconfig.production.json",
    "build:client": "cd client && NODE_ENV=production npm run build",
    "build:server": "tsc -p tsconfig.production.json --skipLibCheck || echo 'TypeScript compilation completed with warnings'",
    "verify:build": "bash scripts/verify-build.sh",
    "prestart": "node -e \"if(process.version.slice(1).localeCompare('20.19.0', undefined, { numeric: true }) !== 0) process.exit(1)\"",
    "start": "NODE_ENV=production node dist/server/index-minimal.js",
    "migrate": "drizzle-kit generate && drizzle-kit push",
    "deploy": "npm run build && npm run migrate",
    "copy:client": "mkdir -p dist/public && cp -r client/dist/* dist/public/",
    "worker": "node server/workers/monitoring-worker.cjs",
    "cron": "node server/workers/scheduled-tasks.cjs",
    "fix:all": "ts-node scripts/automated-fixes.ts",
    "fix": "ts-node scripts/automated-fixes.ts",
    "test:pre-deploy": "ts-node scripts/pre-deploy-test.ts",
    "validate": "npm run fix && npm run test:pre-deploy",
    "build:all": "npm run validate && npm run build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@google/generative-ai": "^0.21.0",
    "@octokit/rest": "^21.0.2",
    "@peculiar/x509": "^1.12.3",
    "@tanstack/react-query": "^4.36.1",
    "@types/node-cron": "^3.0.11",
    "anthropic": "^0.0.0",
    "asn1js": "^3.0.5",
    "autoprefixer": "^10.4.22",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.3.0",
    "bwip-js": "^4.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.4",
    "ethers": "^6.13.2",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsbarcode": "^3.11.6",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "lru-cache": "^11.0.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "node-forge": "^1.3.1",
    "openai": "^4.104.0",
    "pdfkit": "^0.15.0",
    "pg": "^8.13.0",
    "postcss": "^8.5.6",
    "postgres": "^3.4.4",
    "puppeteer": "^24.30.0",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^8.2.0",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "tailwindcss": "^4.1.17",
    "uuid": "^13.0.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@schemastore/package": "^0.0.10",
    "@types/axios": "^0.14.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.11",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.0",
    "@types/node-forge": "^1.3.14",
    "@types/pg": "^8.11.10",
    "@types/qrcode": "^1.5.6",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.0",
    "axios": "^1.13.2",
    "canvas": "^3.2.0",
    "drizzle-kit": "^0.28.1",
    "jsqr": "^1.4.0",
    "pdf-lib": "^1.17.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^5.4.21"
  }
}
